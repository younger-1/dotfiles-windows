{
  plugin: {
    git: {
      enabled: true,
    },
    node: {
      setting: {
        yarnRegistry: null,
      },
    },
    overwall: {
      enabled: true,
    },
  },
  app: {
    autoStart: {
      enabled: true,
    },
  },
  server: {
    intercepts: {
      'raw.githubusercontent.com': {
        '.*': {
          proxy: 'raw.fastgit.org',
        },
      },
    },
  },
}